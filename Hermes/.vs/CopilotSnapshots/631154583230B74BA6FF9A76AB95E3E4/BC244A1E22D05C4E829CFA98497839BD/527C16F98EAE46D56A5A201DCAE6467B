using System.Collections.ObjectModel;
using Hermes.Models;

namespace Hermes.ViewModels
{
    public class PropiedadesViewModel : BaseViewModel
    {
        private ObservableCollection<Propiedad> _propiedades = new();

        public ObservableCollection<Propiedad> Propiedades
        {
            get => _propiedades;
            set => SetProperty(ref _propiedades, value);
        }

        public PropiedadesViewModel()
        {
            // Cargar propiedades
        }
    }
}
